<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.bizi.study.model.uniorder.SubOrder" table="SUB_ORDER" catalog="uniorder">
        <id name="id" type="java.lang.Long">
            <column name="Id" />
            <generator class="assigned" />
        </id>
        <many-to-one name="orderInfo" class="com.bizi.study.model.uniorder.OrderInfo" fetch="select">
            <column name="order_no">
                <comment>关联大订单</comment>
            </column>
        </many-to-one>
        <property name="name" type="java.lang.String">
            <column name="name" length="200" not-null="true">
                <comment>水果名称</comment>
            </column>
        </property>

        <property name="subTotal" type="java.lang.Float">
			<column name="sub_total" precision="7" not-null="true">
				<comment>小计</comment>
			</column>
		</property>
    </class>
</hibernate-mapping>
